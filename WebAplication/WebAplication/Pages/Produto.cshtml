@page
@model WebAplication.Pages.ProdutoModel
@{
    ViewData["Title"] = "Cadastro de Produtos";
}
<h2>@ViewData["Title"]</h2>
<table>
    <thead>
        <tr>
            <th>Nome do Produto</th>
            <th>Preço</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var produto in Model.Produtos)
        {
            <tr>
                <td>@produto.Descricao</td>
                <td>@produto.Preco.ToString("C")</td>
            </tr>
        }
    </tbody>
</table>
<h2>@ViewData["Teste"]</h2>
<p>Estrou testando os dados da minha página</p>
<p>Nome do produto: @Model.Descricao</p>
<p>Preço: @Model.Preco</p>